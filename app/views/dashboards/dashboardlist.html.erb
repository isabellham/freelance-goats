<div class="container-dashboard">
  <div class="card-dashboard">

    <h2>Here are our bookings</h2>

    <ol>
      <% @bookings.each do |booking| %>
        <li><%= link_to "#{booking.goat.name} #{booking.start_date} - #{booking.end_date}" , booking_path(booking) %></li>
      <% end %>
    </ol>
  </div>
  <div class="card-dashboard">
    <h2>Here are our goats</h2>

    <ol>
      <% @goats.each do |goat| %>
        <li><%= link_to "#{goat.name} #{goat.gender} #{goat.goattype}", goat_path(goat) %></li>
      <% end %>
  </div>
</div>
